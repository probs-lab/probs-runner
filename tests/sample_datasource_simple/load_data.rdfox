prefix ufrd: <http://w3id.org/probs-lab/ontology/raw_data/>
prefix simple: <http://w3id.org/probs-lab/ontology/data/simple/>

dsource register "Simple"                                           \
    type    delimitedFile                                           \
    file    "$(dir.datasource)/data.csv"                            \
    header  true                                                    \
    quote   '"'

tupletable create ufrd:simple                                   \
    dataSourceName  "Simple"                                    \
    "columns"       2                                           \
    "1"             "{Object}"                                  \
    "1.datatype"    "string"                                    \
    "2"             "{Value}"                                   \
    "2.datatype"    "xsd:double"                                \
    "2.if-empty"    "absent"
